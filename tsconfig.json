{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist",
    "lib":[
      "es2017",
      "dom",
      "esnext.asynciterable"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}